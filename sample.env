# Copy this file to .env and fill in your actual values

# Get your bot token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Get your Groq API keys from https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here
GROQ_API_KEY_2=your_second_groq_api_key_here

# Telegram group ID for error logging (create a private group and add your bot)
LOG_GROUP_ID=your_log_group_id_here
